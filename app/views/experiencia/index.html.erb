<h1>Listing experiencia</h1>

<table>
  <thead>
    <tr>
      <th>Lugar</th>
      <th>Cargo</th>
      <th>Jefe</th>
      <th>Tel</th>
      <th>Fchini</th>
      <th>Fchfin</th>
      <th>Motivo</th>
      <th>Empleado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiencia.each do |experiencium| %>
      <tr>
        <td><%= experiencium.lugar %></td>
        <td><%= experiencium.cargo %></td>
        <td><%= experiencium.jefe %></td>
        <td><%= experiencium.tel %></td>
        <td><%= experiencium.fchini %></td>
        <td><%= experiencium.fchfin %></td>
        <td><%= experiencium.motivo %></td>
        <td><%= experiencium.empleado %></td>
        <td><%= link_to 'Show', experiencium %></td>
        <td><%= link_to 'Edit', edit_experiencium_path(experiencium) %></td>
        <td><%= link_to 'Destroy', experiencium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Experiencium', new_experiencium_path %>
